    <CompositeTaskInformation>
        <TaskID>VisitTreesWithin100m</TaskID>
        <TaskDescription>Ignoring start and end points, visit as many points as you can in 100 meters and take pictures of each tree.</TaskDescription>
    </CompositeTaskInformation>

    <AtomicTasks>
        <!-- Tree 1 -->
        <AtomicTask>
            <TaskID>MoveTo_Tree_01</TaskID>
            <TaskDescription>Drive to pistachio tree 1 at (-120.4201625, 37.266406).</TaskDescription>
            <Action>
                <ActionType>moveToGPSLocation</ActionType>
                <moveToGPSLocation>
                    <latitude>37.266406</latitude>
                    <longitude>-120.4201625</longitude>
                </moveToGPSLocation>
            </Action>
        </AtomicTask>
        <AtomicTask>
            <TaskID>TakeThermalPic_Tree_01</TaskID>
            <TaskDescription>Capture thermal images of pistachio tree 1.</TaskDescription>
            <Action>
                <ActionType>takeThermalPicture</ActionType>
                <takeThermalPicture>
                    <numberOfPictures>3</numberOfPictures>
